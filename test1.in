// Declarar el modelo de dos vías
model miModelo = (4.7, [-7.4, 1.2, 0.58], [5.4, -9.2], [
    [[15.9, -4.0, 66.1], [-6.1, 3.9], [4.2, -1.3, 12.4, 5.8]],
    [[23.8, 4.5, 3.6], [-0.5, 2.2], [-5.6]]
]);

// Imprimir datos del modelo
print(miModelo.total_datos());
